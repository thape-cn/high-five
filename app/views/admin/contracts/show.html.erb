<% content_for :head do %>
  <%= append_javascript_pack_tag 'stimulus' %>
<% end %>

<div class="row justify-content-center">
  <div class="col-lg-12">
    <div class="container">
      <table class="table table-striped table-bordered">
        <thead>
          <tr>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td><strong>合同类型：</strong> <%= @contract.contract_type %></td>
            <td><strong>合同性质：</strong> <%= @contract.contract_nature %></td>
            <td><strong>主合同编号：</strong> </td>
            <td><strong>主合同名称：</strong> </td>
          </tr>
          <tr>
            <td><strong>来源商机编号：</strong> </td>
            <td><strong>来源商机名称：</strong> </td>
            <td><strong>工程编号：</strong> </td>
            <td><strong>工程名称：</strong> </td>
          </tr>
          <tr>
            <td><strong>东至：</strong> <%= @contract.east_boundary %></td>
            <td><strong>南至：</strong> <%= @contract.south_boundary %></td>
            <td><strong>西至：</strong> <%= @contract.west_boundary %></td>
            <td><strong>北至：</strong> <%= @contract.north_boundary %></td>
          </tr>
          <tr>
            <td><strong>合同编号：</strong> </td>
            <td><strong>合同名称：</strong> <%= @contract.contract_name %></td>
            <td><strong>项目用地性质：</strong> <%= @contract.project_land_use_nature %></td>
            <td><strong>线索提供人：</strong> </td>
          </tr>
          <tr>
            <td><strong>币种：</strong> <%= @contract.currency %></td>
            <td><strong>合同金额（元）:</strong> <%= @contract.contract_amount %></td>
            <td><strong>切分合同金额（元）:</strong> <%= @contract.split_contract_amount %></td>
            <td><strong>实际合同金额：</strong> <%= @contract.actual_contract_amount %></td>
          </tr>
          <tr>
            <td><strong>甲方：</strong> <%= @contract.party_a %></td>
            <td><strong>乙方：</strong> <%= @contract.party_b %></td>
            <td><strong></strong> </td>
            <td><strong></strong> </td>
          </tr>
          <tr>
            <td><strong>丙方：</strong> </td>
            <td><strong>建设单位：</strong> <%= @contract.developer_name %></td>
            <td><strong>规模类型：</strong> <%= @contract.scale_type %></td>
            <td><strong>规模面积：</strong> <%= @contract.scale_area %></td>
          </tr>
          <tr>
            <td><strong>商务责任公司：</strong> </td>
            <td><strong>商务责任部门：</strong> </td>
            <td><strong>商务负责人：</strong> </td>
            <td><strong>商务协助人：</strong> </td>
          </tr>
          <tr>
            <td><strong>收款银行账户：</strong> <%= @contract.beneficiary_bank_account %></td>
            <td><strong>收款开户行：</strong> <%= @contract.beneficiary_bank %></td>
            <td><strong></strong> </td>
            <td><strong></strong> </td>
          </tr>
          <tr>
            <td><strong>是否 EPC:</strong> <%= @contract.is_epc %></td>
            <td><strong>是否设计总承包：</strong> <%= @contract.is_design_general_contractor %></td>
            <td><strong>协议有效期：</strong> </td>
            <td><strong></strong> </td>
          </tr>
          <tr>
            <td><strong>协议适用范围：</strong> </td>
            <td><strong>项目标签：</strong> <%= @contract.project_tag %></td>
            <td><strong></strong> </td>
            <td><strong></strong> </td>
          </tr>
        </tbody>
      </table>      
    </div>
  </div>
</div>
